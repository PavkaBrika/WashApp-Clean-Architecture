<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WashApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">#51b3ff</item>
        <item name="colorPrimaryVariant">@color/statusbarcolor</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>
    <style name="DialogActivities" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:background">@drawable/dialog_bg</item>
        <item name="android:backgroundTint">@color/actionbarcolor</item>
    </style>
        
</resources>